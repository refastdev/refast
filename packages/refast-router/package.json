{
  "name": "@refastdev/refast-router",
  "version": "0.0.0",
  "description": "refast-router",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components.d.ts"
      ],
      "hooks": [
        "./dist/hooks.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./components": {
      "import": {
        "types": "./dist/components.d.ts",
        "default": "./dist/components/index.js"
      }
    },
    "./hooks": {
      "import": {
        "types": "./dist/hooks.d.ts",
        "default": "./dist/hooks/index.js"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/refastdev/refast"
  },
  "homepage": "https://github.com/refastdev/refast#readme",
  "bugs": "https://github.com/refastdev/refast/issues",
  "authors": [
    "luoluoqixi <luoqixi@luoqixi.com> (https://github.com/luoluoqixi)"
  ],
  "keywords": [
    "refast",
    "refast-dev",
    "refast-router",
    "react",
    "fast",
    "fastjs",
    "vite",
    "router",
    "routes",
    "nextjs"
  ],
  "dependencies": {
    "@generouted/react-router": "^1.18.2"
  },
  "devDependencies": {
    "@refastdev/config-typescript": "workspace:*",
    "@refastdev/config-vite": "workspace:*",
    "@refastdev/eslint-config": "workspace:*",
    "@types/react": "^18.2.48",
    "react": ">=18",
    "tsup": "^8.0.1",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-router-dom": "^6.21.3"
  },
  "scripts": {
    "build": "pnpm run lint && tsup --clean",
    "lint": "npx eslint src"
  }
}
